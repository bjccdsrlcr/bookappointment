<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bjccdsrlcr.appoint.dao.AdminDao">
    <select id="queryAdmin" resultType="com.bjccdsrlcr.appoint.entity.Admin">
        SELECT  admin_id,
                password
        FROM innodb.admin
        WHERE admin_id = #{adminId} AND password = #{password}
    </select>

</mapper>